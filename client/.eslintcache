outer>\n        <div className=\"flex-column justify-flex-start min-100-vh\">\n          <Header />\n          <div className=\"container\">\n            <Routes>\n              <Route \n                path=\"/\" \n                element={<Home />} \n              />\n              <Route \n                path=\"/login\" \n                element={<Login />} \n              />\n              <Route \n                path=\"/signup\" \n                element={<Signup />} \n              />\n              <Route \n                path=\"/profile\" \n                element={<Profile />} \n              />\n              <Route \n                path=\"*\" \n                element={<NoMatch />} \n              />\n            </Routes>\n          </div>\n          <Footer />\n        </div>\n      </Router>\n    </ApolloProvider>\n  );\n}\n\nexport default App;\n","/Users/zachburkhart/Desktop/projects/project-content-profile/oracle/client/src/reportWebVitals.js",[],"/Users/zachburkhart/Desktop/projects/project-content-profile/oracle/client/src/pages/Login.js",["74"],"import React, { useState } from 'react';\nimport { useMutation } from '@apollo/client';\n// import { LOGIN_USER } from '../utils/mutations';\n\nimport Auth from '../utils/auth';\n\nconst Login = () => {\n  const [formState, setFormState] = useState({ email: '', password: '' });\n  const [login, { error }] = useMutation(LOGIN_USER);\n\n  // update state based on form input changes\n  const handleChange = (event) => {\n    const { name, value } = event.target;\n\n    setFormState({\n      ...formState,\n      [name]: value,\n    });\n  };\n\n  // submit form\n  const handleFormSubmit = async (event) => {\n    event.preventDefault();\n\n    try {\n      const { data } = await login({\n        variables: { ...formState },\n      });\n\n      Auth.login(data.login.token);\n    } catch (e) {\n      console.error(e);\n    }\n\n    // clear form values\n    setFormState({\n      email: '',\n      password: '',\n    });\n  };\n\n  return (\n    <main className=\"flex-row justify-center mb-4\">\n      <div className=\"col-12 col-md-6\">\n        <div className=\"card\">\n          <h4 className=\"card-header\">Login</h4>\n          <div className=\"card-body\">\n            <form onSubmit={handleFormSubmit}>\n              <input\n                className=\"form-input\"\n                placeholder=\"Your email\"\n                name=\"email\"\n                type=\"email\"\n                id=\"email\"\n                value={formState.email}\n                onChange={handleChange}\n              />\n              <input\n                className=\"form-input\"\n                placeholder=\"******\"\n                name=\"password\"\n                type=\"password\"\n                id=\"password\"\n                value={formState.password}\n                onChange={handleChange}\n              />\n              <button className=\"btn d-block w-100\" type=\"submit\">\n                Submit\n              </button>\n            </form>\n\n            {error && <div>Login failed</div>}\n          </div>\n        </div>\n      </div>\n    </main>\n  );\n};\n\nexport default Login;\n","/Users/zachburkhart/Desktop/projects/project-content-profile/oracle/client/src/pages/Signup.js",[],"/Users/zachburkhart/Desktop/projects/project-content-profile/oracle/client/src/pages/Home.js",["75","76"],"import React from 'react';\nimport SearchForm from '../components/SearchForm';\nimport Searched from '../components/Searched/index';\nimport Queue from '../components/Queue';\n\nimport Auth from '../utils/auth';\nimport { useQuery } from '@apollo/client';\nimport { QUERY_QUEUE, QUERY_ME_BASIC } from '../utils/queries';\n\nconst Home = () => {\n  const { loading, data } = useQuery(QUERY_QUEUE);\n  const { data: userData } = useQuery(QUERY_ME_BASIC);\n  const streamed = data?.Queue || [];\n\n  const loggedIn = Auth.loggedIn();\n\n  return (\n    <main>\n      <div className=\"flex-row justify-space-between\">\n        {loggedIn && (\n          <div className=\"col-12 mb-3\">\n            <SearchForm />\n          </div>\n        )}\n        <div className={`col-12 mb-3 ${loggedIn && 'col-lg-8'}`}>\n          {loading ? (\n            <div>Loading...</div>\n          ) : (\n            <Queue\n              queue={queue}\n              title=\"Your Queue\"\n            />\n          )}\n        </div>\n        {loggedIn && userData ? (\n          <div className=\"col-12 col-lg-3 mb-3\">\n            <Searched\n              Searched={Searched}\n              title=\"Search History\"\n            />\n          </div>\n        ) : null}\n      </div>\n    </main>\n  );\n};\n\nexport default Home;\n","/Users/zachburkhart/Desktop/projects/project-content-profile/oracle/client/src/components/Nav/index.js",["77","78","79","80","81"],"import React from \"react\";\nimport Auth from \"../../utils/auth\";\nimport { Link } from \"react-router-dom\";\nimport Signup from '../Signup/Form';\n\nfunction Nav() {\n    function showNavigation() {\n        if (Auth.loggedIn()) {\n            return (\n                <ul className=\"flex-row\">\n                    <li>\n                        <link to=\"/searchHistory\">\n                            Search History\n                        </link>\n                    </li>\n                    <li>\n                        <link to=\"watchHistory\">\n                            Watch History\n                        </link>\n                    </li>\n                    <li className=\"mx-1\">\n                        {/* this is not using the Link component to logout or user and then refresh the application to the start */}\n                        <a href=\"/\" onClick={() => Auth.logout()}>\n                            Logout\n                        </a>\n                    </li>\n                </ul>\n            );\n        }\n        else {\n            return (\n                <ul className=\"flex-row\">\n                    <li className=\"mx-1\">\n                        <button onClick={openModal}>Sign up</button>\n                        <Modal\n                        isOpen={modalIsOpen}\n                        onAfterOpen={afterOpenModal}\n                        contentLabel=\"Sign up\">\n                            <h2>Sign up today!</h2>\n                            <button onClick={closeModal}>close</button>\n                            <Signup></Signup>\n                        </Modal>\n                    </li>\n                    <li className=\"mx-1\">\n                        <Link to=\"/login\">\n                            Login\n                        </Link>\n                    </li>\n                </ul>\n            );\n        }\n    }\n    return (\n        <header className=\"flex-row px-1\">\n\n            <nav>\n                {showNavigation()}\n            </nav>\n        </header>\n    );\n}\n\nexport default Nav;","/Users/zachburkhart/Desktop/projects/project-content-profile/oracle/client/src/components/Search/index.js",["82","83","84","85","86","87"],"import React from \"react\";\nimport Auth from \"../../utils/auth\";\nimport { Link, renderMatches } from \"react-router-dom\";\nimport axios from 'axios';\nimport mediaSearch from '../../utils/queries';\n\nfunction SearchBar() {\n   $(\"#submit\").click(function(event){\n    event.preventDefault();\n    var search = document.getElementById('search').value;\n    var mediares = mediaSearch(search);\n   })\n\n\n    return (\n        <div className=\"search\">\n            <form id=\"user-form\">\n                <label>Media you are searching for</label>\n                <input id=\"search\" type=\"text\"></input>\n                <input id=\"submit\" type=\"submit\" value=\"submit\"></input>\n            </form>\n        </div>\n    )\n}\n\nexport default SearchBar;","/Users/zachburkhart/Desktop/projects/project-content-profile/oracle/client/src/pages/Profile.js",["88","89","90","91","92","93","94","95"],"import React from 'react';\nimport { Navigate, useParams } from 'react-router-dom';\n\nimport Searched from '../components/Searched';\nimport Queue from '../components/Queue';\nimport SearchForm from '../components/SearchForm';\n\nimport { useQuery, useMutation } from '@apollo/client';\nimport { QUERY_USER, QUERY_ME } from '../utils/queries';\nimport Auth from '../utils/auth';\n\nconst Profile = (props) => {\n  const { username: userParam } = useParams();\n\n  \n  const { loading, data } = useQuery(userParam ? QUERY_USER : QUERY_ME, {\n    variables: { username: userParam },\n  });\n\n  const user = data?.me || data?.user || {};\n\n  // navigate to personal profile page if username is yours\n  if (Auth.loggedIn() && Auth.getProfile().data.username === userParam) {\n    return <Navigate to=\"/profile:username\" />;\n  }\n\n  if (loading) {\n    return <div>Loading...</div>;\n  }\n\n  if (!user?.username) {\n    return (\n      <h4>\n        You need to be logged in to see this. Use the navigation links above to\n        sign up or log in!\n      </h4>\n    );\n  }\n\n  const handleClick = async () => {\n    try {\n      await addFriend({\n        variables: { id: user._id },\n      });\n    } catch (e) {\n      console.error(e);\n    }\n  };\n\n  return (\n    <div>\n      <div className=\"flex-row mb-3\">\n        <h2 className=\"bg-dark text-secondary p-3 display-inline-block\">\n          Viewing {userParam ? `${user.username}'s` : 'your'} profile.\n        </h2>\n\n        {userParam && (\n          <button className=\"btn ml-auto\" onClick={handleClick}>\n            Add Friend\n          </button>\n        )}\n      </div>\n\n      <div className=\"flex-row justify-space-between mb-3\">\n        <div className=\"col-12 mb-3 col-lg-8\">\n          <ThoughtList\n            thoughts={user.thoughts}\n            title={`${user.username}'s thoughts...`}\n          />\n        </div>\n\n        <div className=\"col-12 col-lg-3 mb-3\">\n          <FriendList\n            username={user.username}\n            friendCount={user.friendCount}\n            friends={user.friends}\n          />\n        </div>\n      </div>\n      <div className=\"mb-3\">{!userParam && <ThoughtForm />}</div>\n    </div>\n  );\n};\n\nexport default Profile;\n","/Users/zachburkhart/Desktop/projects/project-content-profile/oracle/client/src/utils/auth.js",[],"/Users/zachburkhart/Desktop/projects/project-content-profile/oracle/client/src/components/Searched/index.js",[],"/Users/zachburkhart/Desktop/projects/project-content-profile/oracle/client/src/components/Signup/Form.js",[],"/Users/zachburkhart/Desktop/projects/project-content-profile/oracle/client/src/utils/queries.js",[],"/Users/zachburkhart/Desktop/projects/project-content-profile/oracle/client/src/utils/mutations.js",[],"/Users/zachburkhart/Desktop/projects/project-content-profile/oracle/client/src/components/Queue/index.js",[],{"ruleId":"96","severity":1,"message":"97","line":11,"column":8,"nodeType":"98","messageId":"99","endLine":11,"endColumn":12},{"ruleId":"96","severity":1,"message":"100","line":12,"column":8,"nodeType":"98","messageId":"99","endLine":12,"endColumn":11},{"ruleId":"96","severity":1,"message":"101","line":13,"column":8,"nodeType":"98","messageId":"99","endLine":13,"endColumn":14},{"ruleId":"102","severity":2,"message":"103","line":48,"column":12,"nodeType":"104","messageId":"105","endLine":48,"endColumn":18},{"ruleId":"102","severity":2,"message":"106","line":69,"column":27,"nodeType":"104","messageId":"105","endLine":69,"endColumn":34},{"ruleId":"102","severity":2,"message":"107","line":73,"column":12,"nodeType":"104","messageId":"105","endLine":73,"endColumn":18},{"ruleId":"108","severity":2,"message":"109","line":9,"column":42,"nodeType":"98","messageId":"110","endLine":9,"endColumn":52},{"ruleId":"96","severity":1,"message":"111","line":13,"column":9,"nodeType":"98","messageId":"99","endLine":13,"endColumn":17},{"ruleId":"108","severity":2,"